     async loadRequests(){\n            this.isLoading = true;\n            try{\n            await this.$store.dispatch('requests/fetchRequests');\n            } catch (error){\n                this.error = error.message || 'something failed';\n            }\n                this.isLoading = false;\n        },\n    \n    handleError(){\n        this.error = null;\n    }\n}}\n</script>\n\n\n<style scoped>\n    header {\n  text-align: center;\n}\n\nul {\n  list-style: none;\n  margin: 2rem auto;\n  padding: 0;\n  max-width: 30rem;\n}\n\nh3 {\n  text-align: center;\n}\n</style>\n\n"]}]}